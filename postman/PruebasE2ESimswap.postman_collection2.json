							"",
							"",
							"        ]",
							"    }",
							"},",
							"(err, res) => {",
							"    if (err) {",
							"        console.log(err);",
							"    } else {",
							"        pm.variables.set(\"access_token\", res.json().access_token);",
							"        console.log(\"Token: \", res.json().access_token);",
							"        console.log(\"Token: \", res.json());",
							"        console.log(\"access_token: \", pm.variables.get(\"access_token\"))",
